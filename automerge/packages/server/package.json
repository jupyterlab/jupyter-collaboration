{
  "name": "@jupyterlab/automerge-server",
  "version": "0.1.0",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc",
    "build:tsc": "tsc --build",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "clean:all": "rimraf node_modules lib tsconfig.tsbuildinfo",
    "prepublishOnly": "npm run build",
    "watch": "tsc -w --listEmittedFiles",
    "hello": "ts-node src/index.ts",
    "test": "jest",
    "test:automerge": "jest src/automerge.test.js",
    "test:frontend": "jest src/frontend.test.js",
    "server:ws:start": "ts-node src/ws-perf/AutomergeServer.ts",
    "server:ws:wait": "wait-on tcp:4321"
  },
  "dependencies": {
    "automerge-wasm-node": "file:./../../externals/automerge-wasm-nodejs",
    "universal-websocket-client": "1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/expect-puppeteer": "^4.4.5",
    "@types/jest": "26.0.19",
    "@types/jest-environment-puppeteer": "^4.4.1",
    "@types/puppeteer": "^5.4.2",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "jest-puppeteer": "^4.4.0",
    "puppeteer": "^5.5.0",
    "ts-node": "9.0.0",
    "typescript": "4.1.3",
    "wait-on": "5.0.1",
    "ws": "7.3.1"
  }
}
