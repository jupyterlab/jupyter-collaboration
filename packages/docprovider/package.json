{
  "name": "@jupyterlab/docprovider",
  "version": "4.0.0-alpha.19",
  "description": "JupyterLab - Document Provider",
  "homepage": "https://github.com/jupyter-server/jupyter_server_ydoc",
  "bugs": {
    "url": "https://github.com/jupyter-server/jupyter_server_ydoc/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyter-server/jupyter_server_ydoc.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "sideEffects": [
    "style/**/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "style/index.js"
  ],
  "scripts": {
    "build": "tsc -b",
    "build:prod": "jlpm run build",
    "build:test": "tsc --build tsconfig.test.json",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "clean:all": "rimraf lib tsconfig.tsbuildinfo node_modules",
    "docs": "typedoc src",
    "install:extension": "jlpm run build",
    "test": "jest",
    "test:cov": "jest --collect-coverage",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:debug:watch": "node --inspect-brk node_modules/.bin/jest --runInBand --watch",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyter/ydoc": "^0.3.1",
    "@jupyterlab/coreutils": "^6.0.0-alpha.18",
    "@jupyterlab/services": "^7.0.0-alpha.18",
    "@lumino/coreutils": "^2.0.0-alpha.6",
    "@lumino/disposable": "^2.0.0-alpha.6",
    "@lumino/signaling": "^2.0.0-alpha.6",
    "y-protocols": "^1.0.5",
    "y-websocket": "^1.3.15",
    "yjs": "^13.5.40"
  },
  "devDependencies": {
    "@jupyterlab/testing": "^4.0.0-alpha.18",
    "@types/jest": "^29.2.0",
    "rimraf": "~3.0.0",
    "typescript": "~4.7.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
